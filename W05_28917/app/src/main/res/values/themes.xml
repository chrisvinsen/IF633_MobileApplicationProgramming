<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.W05_28917" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="seekBarRed" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressTint">@color/red</item>
        <item name="colorControlActivated">@color/red</item>
    </style>
    <style name="seekBarGreen" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressTint">@color/green</item>
        <item name="colorControlActivated">@color/green</item>
    </style>
    <style name="seekBarBlue" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressTint">@color/blue</item>
        <item name="colorControlActivated">@color/blue</item>
    </style>
</resources>